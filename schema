table seq
col lastval integer

table nodes
col id integer
col mac varchar
col last_ts timestamp

table sessions
col session_id varchar
col session varchar
col updated timestamp

table users
col user_id integer
col email varchar
col passwd varchar

table coops
col coop_id integer
col coop_name varchar

table hens
col hen_id integer
col hen_name varchar
col coop_id integer

table chicks
col chick_id integer
col chick_name varchar
col hen_id integer
