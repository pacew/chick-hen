table seq
col lastval integer

table nodes
col id integer
col mac varchar
col last_ts timestamp

table sessions
col session_id varchar
col session varchar
col updated timestamp

table users
col user_id integer
col email varchar
col passwd varchar

table hens
col hen_id integer
col hen_name varchar
col key_text varchar
col key varchar

table chicks
col chick_mac varchar
col chick_name varchar
col hen_id integer
col chanlist_id integer

table reported_chicks
col hen_name varchar
col chick_mac varchar

table hwchan
col hwchan_id integer
col chanlist_id integer
col sort_order integer
col hwchan_name varchar
col chan_type integer
col port integer
col bit_width integer
col bit_position integer

table chanlists
col chanlist_id integer
col chanlist_name varchar

